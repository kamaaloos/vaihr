{
  "name": "vaihtoratti",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "engines": {
    "node": ">=20.8.1"
  },
  "scripts": {
    "start": "expo start",
    "test": "jest",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "migrate": "ts-node scripts/migrate.ts",
    "migrate:otp": "node scripts/run-otp-migration.js"
  },
  "jest": {
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.test.[jt]s?(x)"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@rneui/.*)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ]
  },
  "dependencies": {
    "@expo/config-plugins": "~54.0.1",
    "@expo/metro-config": "~54.0.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^7.8.11",
    "@react-navigation/native": "^7.1.24",
    "@react-navigation/native-stack": "^7.8.5",
    "@react-navigation/stack": "^7.6.11",
    "@supabase/supabase-js": "^2.48.1",
    "@types/uuid": "^10.0.0",
    "base64-arraybuffer": "^1.0.2",
    "date-fns": "^2.30.0",
    "expo": "~54.0.25",
    "expo-asset": "~12.0.10",
    "expo-av": "~16.0.7",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.18",
    "expo-device": "~8.0.9",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.19",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-notifications": "~0.32.13",
    "expo-print": "~15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "expo-video": "~3.0.14",
    "immer": "^10.1.1",
    "metro": "^0.83.1",
    "metro-config": "^0.82.0",
    "metro-core": "^0.82.0",
    "metro-resolver": "^0.82.0",
    "metro-runtime": "^0.82.0",
    "metro-source-map": "^0.82.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-blob-util": "^0.19.6",
    "react-native-emoji-selector": "^0.2.0",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-paper": "^5.12.3",
    "react-native-paper-dates": "^0.22.3",
    "react-native-pdf": "^6.7.6",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-size-matters": "^0.4.2",
    "react-native-toast-message": "^2.2.0",
    "react-native-websocket": "^1.0.2",
    "react-native-webview": "13.15.0",
    "rn-emoji-keyboard": "^1.7.0",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.17",
    "@types/react": "~19.1.10",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-native-gesture-handler": "~2.28.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "base64-arraybuffer",
          "immer",
          "nodemailer",
          "pg",
          "sharp",
          "uuid",
          "react-native-emoji-selector",
          "metro",
          "metro-core",
          "metro-runtime",
          "metro-source-map"
        ],
        "listUnknownPackages": false
      }
    }
  }
}
